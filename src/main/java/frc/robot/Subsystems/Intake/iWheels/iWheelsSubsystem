package frc.robot.Subsystems.Intake.IntakeWheels;

import edu.wpi.first.wpilibj2.command.SubsystemBase;
import frc.robot.Constants.RobotMap;
import com.revrobotics.CANSparkMax;
import com.revrobotics.CANSparkLowLevel.MotorType;

public class iWheelsSubsystem extends SubsystemBase {
  
  private static iWheelsSubsystem instance;
  private CANSparkMax motorIW;

  private iWheelsSubsystem() {
    motorIW = new CANSparkMax(RobotMap.INTAKE_WHEELS_MOTOR_ID, MotorType.kBrushless);
  }

  public static iWheelsSubsystem getInstance() {
    if (instance == null){
      instance = new iWheelsSubsystem();
    }
    return instance;
  }
  public void setSpeed(double speed) {
  motorIW.set(speed);
  }
  
  @Override
  public void periodic() {} 
}